		function toggle(num) {
			var ele = document.getElementById("toggleText" + num);
			var text = document.getElementById("displayText" + num);
			if(ele.style.display == "block") {
		    		ele.style.display = "none";
				text.innerHTML = "view";
		  	}
			else {
				ele.style.display = "block";
				text.innerHTML = "hide";
			}
		} 
